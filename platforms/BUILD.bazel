package(default_visibility = ["//visibility:public"])

constraint_setting(name = "libc_version")

exec_kylin_properties = {
    "OSFamily": "kylin-v10",
}

constraint_value(
    name = "glibc_2_23",
    constraint_setting = ":libc_version",
)

platform(
    name = "kylin",
    constraint_values = [
        "@platforms//cpu:x86_64",
        "@platforms//os:linux",
        ":glibc_2_23",
    ],
    exec_properties = exec_kylin_properties,
)
