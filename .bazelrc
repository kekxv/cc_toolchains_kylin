# remote timeout
# allow_empty 新版本默认为True
common                              --incompatible_disallow_empty_glob=False
build                               --incompatible_disallow_empty_glob=False
build                               --remote_timeout=36000000
build                              --cxxopt='-std=c++17'

build:kylin              --remote_instance_name=fuse
build:kylin              --cpu=kylin-x86_64
build:kylin              --platforms=//:kylin
build:kylin              --extra_execution_platforms=//:kylin

